    </sectionGroup>
  </configSections>
  <couchbase-caching>
    <servers>
      <add uri="http://10.141.150.102:8091/"></add>
    </servers>
    <buckets>
      <add name="memcached2"></add>
    </buckets>
  </couchbase-caching>
  <couchbaseClients>
    <couchbase>
      <servers>
        <add uri="http://10.10.40.114:8091"></add>
      </servers>
      <buckets>
        <add name="WebapiCaheDemo"></add>
      </buckets>
    </couchbase>
  </couchbaseClients>
 
